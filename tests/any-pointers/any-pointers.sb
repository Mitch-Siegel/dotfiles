#include "tests-common.sbh"

any *fun funReturningAnyPointer()
{
    return 0xabcdefffabcdefff;
}

fun printAnyPointer(any *a)
{
    u64 asNumber = a;
    printHex(asNumber);
}

fun main()
{
    any *hereBeDragons = funReturningAnyPointer();
    u32 dragons = hereBeDragons;
    printHex(dragons);
    uart_putc('\n');
    printAnyPointer(hereBeDragons);
    uart_putc('\n');
    exit();
