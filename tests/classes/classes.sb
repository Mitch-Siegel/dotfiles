#include "tests-common.sbh"

class Money
{
    u32 dollars;
    u8 cents;
};

void fun PrintMoney(Money *m)
{
    uart_putc('$');
    printNum(m->dollars);
    uart_putc('.');
    printNum(m->cents);
}

void fun main()
{
    Money twelveDollars;
    twelveDollars.dollars = 12;
    twelveDollars.cents = 0;
    PrintMoney(&twelveDollars);
    exit();
}