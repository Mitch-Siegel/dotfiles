#include "tests-common.sb"

class Counter {
    u32 count;
};

impl Counter {
    fun Increment(Counter *self) {
        self->count += 1;
    }

    fun Set(Counter *self, u32 value) {
        self->count = value;
    }
}


fun main() {
    Counter c;
    c.Set(123);
    printHex(c.count);
    uart_putc('\n');
    c.Increment();
    printHex(c.count);
    uart_putc('\n');
    exit();
}
