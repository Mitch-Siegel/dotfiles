#include "tests-common.sb"


fun main()
{
    printStr((u8 *)"This string has a newline\n");
    printStr((u8 *)"This string should have an 'A' after the colon:!\bA\n");
    printStr((u8 *)"\tThis string should be indented by one tab\n");
    printStr((u8 *)"This string should have a double quote after the colon:\"\n");
