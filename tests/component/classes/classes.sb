#include "tests-common.sb"

class Money
{
    u32 dollars;
    u8 cents;
};

fun PrintMoney(Money *m)
{
    uart_putc('$');
    printNum(m.dollars);
    uart_putc('.');
    printNum(m.cents);
}

fun ResetMoney(Money *m)
{
    *m.dollars = 0;
    *m.cents = 0;
}

fun main()
{
    Money twelveDollars;
    twelveDollars.dollars = 12;
    twelveDollars.cents = 0;
    PrintMoney(&twelveDollars);
    ResetMoney(&twelveDollars);
    PrintMoney(&twelveDollars);
    exit();
}
