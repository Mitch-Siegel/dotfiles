//#include "print.h"
#include "uart.h"

u32 fun fib(u8 n)
{
    if(n > 2)
    {
        return fib(n - 1) + fib(n - 2);
    }
    else
    {
        return n;
    }
}

void fun main()
{
    uart_init();
	uart_putc(65);
    u8 n = 0;
    while(n < 6)
    {
        uart_putc(48 + fib(n));
        n = n + 1;
    }
}