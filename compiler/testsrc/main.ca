#include "uart.h"
#include "print.h"

u32 fun fib(u8 n)
{
    if(n > 1)
    {
        return fib(n - 1) + fib(n - 2);
    }
    else
    {
        return n;
    }
}

void fun main()
{
    uart_init();
	uart_putc(65);
    u8 n = 0;
    while(n < 20)
    {
        u32 result = fib(n);
        u8 smallerResult = result;
        printNum(smallerResult);
        n = n + 1;
    }
}