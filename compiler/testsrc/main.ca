

fun printCheat(uint32 value->void)
{
	asm {
		mov %r0, (%bp+8)
		out 1, %r0
	}
	;
}

fun fibRec(uint8 value->uint32)
{
	if(2 > value)
	{
		return value;
	}
	else
	{
		return fibRec(value - 2) + fibRec(value - 1);
	}
}

fun main(->void)
{
    uint8 a = 6;
    uint8 b = 3;
	uint8 i = 0;
	while(a > b)
	{
		i = 0;
		while(i < 25)
		{
			printCheat(fibRec(i));
			i += 1;
		}
	}

}