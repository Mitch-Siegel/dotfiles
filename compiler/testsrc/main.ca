fun print(uint8 char->void) asm
{
	li t0, 0x10000000
	lbu t1, 4(sp)
	sb t1, 0(t0)
}

uint8 dummy = 0;

fun printStr(uint8 *str->uint32)
{
	uint32 numPrinted;
	numPrinted = 0;
	while(*str != 0)
	{
		print(*str);
		str += 1;
		numPrinted += 1;
	}
	return numPrinted;
}

fun main(->void)
{
	printStr("123");
	print(10);
}