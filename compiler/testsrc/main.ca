

class String
{
    uint8 *data;
    uint32 len;
};

class Price
{
    uint16 dollars;
    uint8 cents;
};


class Item
{
    class String name;
    class Price price;
};

fun printCheat(uint32 value->void)
{
	asm {
		mov %r0, (%bp+8)
		out 1, %r0
	}
	;
}


fun main(->void)
{
    class Item i;
    i.price.dollars = 234;
    i.price.cents = 12;
    printCheat(i.price.dollars);
    printCheat(i.price.cents);
}