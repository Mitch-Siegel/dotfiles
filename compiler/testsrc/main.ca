#include "uart.h"
#include "print.h"

u8 fun scopeTest(u8 a)
{
    if(((a - 1) / 2) == (a / 2))
    {
        u8 b = 123;
        return b;
    }

    return a;
}

void fun main()
{
    uart_init();
	uart_putc(65);
    u8 n = 0;
    while(n < 20)
    {
        printNum(scopeTest(n));
        n = n + 1;
    }
}