

fun printNum(uint32 n->void)
{
	asm{
		mov %r0, (%bp+8)
		out $1, %r0
	};
}


fun fibRec(uint8 n->uint32)
{
	if(n > 1)
	{
		return fibRec(n - 1) + fibRec(n - 2);
	}
	return n;
}

fun main(->void)
{
	uint32 fibs[15];

	uint8 i = 0;
	while(i < 15)
	{
		fibs[i] = fibRec(i);
		i = i + 1;
	}

	i = 0;
	while(i < 15)
	{
		printNum(fibs[i]);
		i = i + 1;
	}
}
