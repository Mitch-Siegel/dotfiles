void fun uart_putc(u8 c)
{
    u8 *uart0 = 268435456;
    *(uart0) = c;
}


u32 fun mod(u32 a, u32 b)
{
	while(a >= b)
	{
		a -= b;
	}
	return a;
}

void fun printNum(u32 value)
{
	u8[10] chars;
	u8 nChars = 0;
	u32 vc = value;
	if(value == 0)
	{
		chars[0] = '0';
		nChars = 1;
	}

	while(value > 0)
	{
		u8 rv = mod(value, 10);
		chars[nChars] = rv + '0';
		value = value / 10;
		nChars = nChars + 1;
		chars[nChars] = 0;
	}
	
	while(nChars > 0)
	{
		uart_putc(chars[nChars - 1]);
		nChars -= 1;
	}
	uart_putc(10);
}

void fun exit() asm
{
    li a0, 0x100000
    li a1, 0x5555
    sw a1,0(a0)
}

void fun main()
{
    u32 value = 0x1234;
    printNum(value);
    value = 0x4D2;
    printNum(value);
    value = 0x4d2;
    printNum(value);
    value = 0xabcdef;
    printNum(value);
	value = 0xABCDEF;
    printNum(value);
	value = 0x1abcdef;
    printNum(value);
	value = 0x1ABCDEF;
    printNum(value);
	value = 0xabcdef1;
    printNum(value);
	value = 0xABCDEF1;
    printNum(value);
    exit();
}

